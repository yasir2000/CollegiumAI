{
  "name": "@collegiumai/sdk",
  "version": "1.0.0",
  "description": "Comprehensive SDK for integrating with the CollegiumAI Framework - AI Multi-Agent Collaborative Platform for Digital Universities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm run build:types",
    "docs": "typedoc",
    "clean": "rimraf dist"
  },
  "keywords": [
    "ai",
    "multi-agent",
    "education",
    "university",
    "blockchain",
    "governance",
    "react-framework",
    "student-services",
    "academic-advisor",
    "compliance",
    "higher-education",
    "digital-university",
    "sdk",
    "api-client"
  ],
  "author": {
    "name": "CollegiumAI Team",
    "email": "support@collegiumai.com",
    "url": "https://collegiumai.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/collegiumai/framework.git"
  },
  "bugs": {
    "url": "https://github.com/collegiumai/framework/issues"
  },
  "homepage": "https://docs.collegiumai.com",
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.14.0",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.2.0"
  },
  "peerDependencies": {
    "web3": "^4.0.0",
    "ethers": "^6.0.0"
  },
  "peerDependenciesMeta": {
    "web3": {
      "optional": true
    },
    "ethers": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{ts,js}",
      "!src/**/*.d.ts",
      "!src/**/*.test.{ts,js}",
      "!src/**/index.{ts,js}"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/__tests__/**/*.{ts,js}",
      "**/*.(test|spec).{ts,js}"
    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "warn",
      "@typescript-eslint/explicit-function-return-type": "warn"
    },
    "env": {
      "node": true,
      "jest": true
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 120,
    "tabWidth": 2,
    "useTabs": false
  }
}